        "jquery": "^3.3.1",
        "laravel-mix": "^1.0",
        "lodash": "^4.17.5",
        "vue": "^2.5.7",
        "ws": "3.3.2"
    },
    "dependencies": {
        "eslint": "^4.17.0",
        "lottie-web": "^5.1.7",
        "noty": "^3.2.0-beta",
        "parsleyjs": "^2.8.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0"
    }
